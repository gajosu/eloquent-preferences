{
    "name": "gajosu/eloquent-preferences",
    "type": "library",
    "description": "Preferences for Laravel Eloquent models",
    "keywords": [
        "laravel",
        "eloquent",
        "preferences"
    ],
    "homepage": "https://github.com/gajosu/eloquent-preferences",
    "license": "MIT",
    "authors": [
        {
            "name": "Kevin Laude",
            "email": "nerfyoda@gmail.com"
        }
    ],
    "require": {
        "ext-json": "*",
        "illuminate/database": "~5|~6.0|~7.0|~8.0|^9.0",
        "illuminate/support": "~5|~6.0|~7.0|~8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Gajosu\\EloquentPreferences\\": "src/"
        }
    },
    "require-dev": {
        "ext-pdo": "*",
        "illuminate/events": "~5|~6.0|~7.0|~8.0|^9.0",
        "phpunit/phpunit": "~5.0|~4.8|^9.5.10",
        "friendsofphp/php-cs-fixer": "^3.8",
        "nunomaduro/collision": "^6.0",
        "nunomaduro/larastan": "^2.0.1",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "autoload-dev": {
        "psr-4": {
            "Gajosu\\EloquentPreferences\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    }
}
